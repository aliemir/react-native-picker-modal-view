{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["."],
                "alias": {
                    "@Components": "./src/Components",
                    "@Enum": "./src/Enum",
                    "@Helpers": "./src/Helpers",
                    "@Interfaces": "./src/Interfaces",
                    "@Styles": "./src/Assets/Styles",
                    "@Images": "./src/Assets/Images"
                }
            }
        ]
    ]
}
